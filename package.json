{
    "name": "gen-env-template",
    "version": "1.0.4",
    "description": "Generate .env template file from .env",
    "main": "src/index.js",
    "scripts": {
        "lint": "eslint --ext .js .",
        "prepare": "husky install",
        "test": "GEN_ENV_TEMPLATE_BASE=$PWD GEN_ENV_TEMPLATE_ABSOLUTE_PATH=$PWD/src/bin/cli.js jest --testTimeout 20000",
        "lint:staged": "lint-staged"
    },
    "bin": {
        "gen-env-template": "src/bin/cli.js"
    },
    "lint-staged": {
        "*.js": ["eslint"]
    },
    "keywords": [
        "dotenv",
        "env",
        "template",
        "generate",
        "cli"
    ],
    "homepage": "https://github.com/mrsauravsahu/gen-env-template",
    "bugs": {
        "email": "mrsauravsahu@outlook.com",
        "url": "https://github.com/mrsauravsahu/gen-env-template"
    },
    "author": "Saurav Sahu <mrsauravsahu@outlook.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "eslint": "^7.25.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.4",
        "shelljs": "^0.8.4"
    }
}
